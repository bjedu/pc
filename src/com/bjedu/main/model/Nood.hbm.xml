<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
 * @version 1.0 2016-04-28 release(<WJK GENERATOR> <-(Write your name as author here))
-->
<hibernate-mapping>
	<class name="com.bjedu.main.model.Nood" table="edu_nood" >
		<id name="uuid" type="java.lang.String">
			<column name="UUID" length="60" />
			<generator class="assigned" />
		</id>
		<property name="nameN" type="java.lang.String">
			<column name="NAME_N" length="50">
				<comment>年度</comment>
			</column>
		</property>
		<property name="iwfId" type="java.lang.String">
			<column name="IWF_ID" length="60">
				<comment>单位标志</comment>
			</column>
		</property>
		<property name="noodType" type="java.lang.String">
			<column name="NOOD_TYPE" length="1">
				<comment>单位标志</comment>
			</column>
		</property>
		<property name="nextId" type="java.lang.String">
			<column name="NEXT_ID" length="60">
				<comment>单位标志</comment>
			</column>
		</property>
		<property name="perId" type="java.lang.String">
			<column name="PER_ID" length="60">
				<comment>单位标志</comment>
			</column>
		</property>
		<property name="noodNo" type="java.lang.Integer">
			<column name="NOOD_NO" >
				<comment>月份</comment>
			</column>
		</property>
		
	</class>
</hibernate-mapping>
